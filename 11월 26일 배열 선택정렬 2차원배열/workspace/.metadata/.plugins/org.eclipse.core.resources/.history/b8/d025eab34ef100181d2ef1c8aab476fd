package array;

public class Test8 {

	public static void main(String[] args) {
		int[][]array= new int[][] {{12, 41, 36, 56}, 
										{82, 10, 12, 61}, 
										{14, 16, 18, 78}, 
										{45, 26, 72, 23}};
		double sum=0;
		for(int i=0; i<array.length;i++) {
			for(int j=0; j<4; j++){//열 길이 표현하는거 까먹음
				sum +=array[i][j];
			}
		}
		System.out.println("합계 : "+sum);
		System.out.println("평균 : "+sum/16);//행길이*열길이하면되는데 열길이 표현 까먹음
	}
}
