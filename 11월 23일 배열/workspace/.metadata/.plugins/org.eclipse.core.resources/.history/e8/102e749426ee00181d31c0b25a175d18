package logic.home.model;

public class StaticMethodSample {
	public static void testMathRandom() {
		int rnum = (int)(Math.random()*45)+1;
		System.out.println("1~45 임의의 정수 출력값 : "+rnum);
	}
	public static void testMathAbs() {
		double rnum = (Math.random()*100000)*100/100.0;
		System.out.println("임의이 실수 "+rnum+"의 절대값 : "+Math.abs(rnum));
		
	}
	public static void testMathMax() {
		
	}
}
