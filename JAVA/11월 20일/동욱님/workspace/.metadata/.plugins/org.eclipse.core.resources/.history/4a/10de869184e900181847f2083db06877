package controll.sample;

import java.util.*;

public class IfSample {
	//if만 사용하는 경우
	private Scanner sc = new Scanner(System.in);
	
	public void unicodeToChar() {
		//정수를 입력받아서 
		//정수를 유니코드로 하는 문자 출력
		//유니코드 : 0 ~ 65535
		System.out.print("유니코드 입력 : ");
		int code = sc.nextInt();
		
		if(code>=0 && code<=65535) {
			System.out.println(code + " -> " + (char)code);
		} // if close
		else
			System.out.println("");
		
		
	}
}
