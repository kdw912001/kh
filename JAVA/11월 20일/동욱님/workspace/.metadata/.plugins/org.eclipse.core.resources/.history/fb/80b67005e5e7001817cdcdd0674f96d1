package var.test;

public class TestVariable {
	public void testFinal() {
		//상수변수
		//1개의 값만 기록해서 사용하는 변수
		//기록된 초기값은 변경 못 하는 변수
		final double pi = 3.14;
		System.out.println("pi : " + pi);
		
		/*pi=99.9;
		System.out.println("pi : " +pi);*/
	}
	
	public static void main(String[] args) {
		TestVariable test = new TestVariable();
		test.testFinal();

	}

}
